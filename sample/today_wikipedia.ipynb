   "source": [
    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "\n",
    "url = \"https://ja.wikipedia.org\"\n",
    "\n",
    "response = requests.get(url)\n",
    "#print(response)\n",
    "\n",
    "soup = BeautifulSoup(response.content, \"html.parser\")\n",
    "#print(soup)\n",
    "\n",
    "today = soup.find(\"div\", attrs={\"id\": \"on_this_day\"}).text\n",
    "print(today)"
   ]
